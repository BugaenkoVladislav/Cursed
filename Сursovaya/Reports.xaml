<Window x:Class="Сursovaya.Reports"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Сursovaya"
        mc:Ignorable="d"
        Title="Отчеты" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="290"/>
            <RowDefinition Height="*"/> 
        </Grid.RowDefinitions>
        <TextBlock Text="Отчеты" TextAlignment="Center" HorizontalAlignment="Center" FontSize="26" FontWeight="Bold" Margin="0,30,0,0"></TextBlock>
        <StackPanel VerticalAlignment="Center">
            <Button Height="40" Width="300" Margin="0,10" Click="ProductsBuy_OnClick">Покупка товаров</Button>
            <Button Height="40" Width="300" Margin="0,10" Click="ProductsProvide_OnClick">Доставка товаров</Button>
        </StackPanel>
    </Grid>
</Window>
